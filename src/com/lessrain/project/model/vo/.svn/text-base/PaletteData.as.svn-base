package com.lessrain.project.model.vo {
	/**
	 * @author lessintern
	 */
	public class PaletteData {
		
		private var _id:String;
		private var _index:int;
		private var _defaultColorIndex:int;
		private var _name:String;
		private var _isDefault:Boolean;
		private var _colors:Vector.<ColorData>;
		
		public function PaletteData() {
			_colors = new Vector.<ColorData>();
		}

		public function get id() : String {
			return _id;
		}

		public function set id(id : String) : void {
			_id = id;
		}

		public function get name() : String {
			return _name;
		}

		public function set name(name : String) : void {
			_name = name;
		}

		public function get colors() : Vector.<ColorData> {
			return _colors;
		}

		public function get isDefault() : Boolean {
			return _isDefault;
		}

		public function set isDefault(isDefault : Boolean) : void {
			_isDefault = isDefault;
		}
		
		public function toString():String{
			return _name;
		}

		public function get index() : int {
			return _index;
		}

		public function set index(index : int) : void {
			_index = index;
		}

		public function get defaultColorIndex() : int {
			return _defaultColorIndex;
		}

		public function set defaultColorIndex(defaultColorIndex : int) : void {
			_defaultColorIndex = defaultColorIndex;
		}
		
	}
}
